cpp = ['clang++', '-arch', 'x86_64', '-mmacosx-version-min=10.13']
ar = 'ar'
ld = 'ld'
strip = 'strip'
lipo = 'lipo'
ranlib = 'ranlib'
pkgconfig = 'pkg-config'
