    <test name="regex-test-4a-2">
      <keystroke key="4" />
      <keystroke key="e" />
      <keystroke key="stampy" />
      <keystroke key="u-0320" />
      <keystroke key="u-0300" />
      <check result="4e\u{0320}\u{0300}" />
