    </test>
    <!-- to match-->
    <test name="regex-test-4a-0x">
      <keystroke key="stampy" />
      <keystroke key="4" />
      <keystroke key="e" />
      <keystroke key="u-0300" />
      <keystroke key="u-0320" />
      <check result="4NO0" />
    </test>
    <test name="regex-test-4a-1x">
      <keystroke key="4" />
      <keystroke key="stampy" />
      <keystroke key="e" />
      <keystroke key="u-0300" />
      <keystroke key="u-0320" />
      <check result="4NO0" />
    </test>
    <test name="regex-test-4a-2x">
      <keystroke key="4" />
      <keystroke key="e" />
      <keystroke key="stampy" />
      <keystroke key="u-0300" />
      <keystroke key="u-0320" />
      <check result="YES4b" />
    </test>
    <test name="regex-test-4a-3x">
      <keystroke key="4" />
      <keystroke key="e" />
      <keystroke key="u-0300" />
      <keystroke key="stampy" />
      <keystroke key="u-0320" />
      <check result="YES4c" />
    </test>
    <test name="regex-test-4a-4x">
      <keystroke key="4" />
      <keystroke key="e" />
      <keystroke key="u-0300" />
      <keystroke key="u-0320" />
      <keystroke key="stampy" />
      <check result="4NOYES0" />
    </test>
  </tests>
  <tests name="regex-tests-5">
    <test name="regex-test-5a-0">
      <keystroke key="stampy" />
      <keystroke key="5" />
      <keystroke key="e" />
      <keystroke key="u-0320" />
      <keystroke key="u-0300" />
      <check result="YES5x" />
    </test>
    <test name="regex-test-5a-1">
      <keystroke key="stampy" />
      <keystroke key="5" />
      <keystroke key="nfc" />
      <check result="5\u{00e8}\u{0320}" />
    </test>
  </tests>
  <tests name="regex-tests-6">
    <test name="regex-test-6a-0">
      <keystroke key="6" />
      <keystroke key="stampy" />
      <keystroke key="e" />
      <keystroke key="u-0320" />
      <backspace /> <!-- skip the number, match any -->
      <check result="YES6" />
    </test>
    <test name="regex-test-6a-1">
      <keystroke key="6" />
      <keystroke key="e" />
      <keystroke key="stampy" />
      <keystroke key="u-0320" />
      <backspace /> <!-- skip the number, match any -->
      <check result="YES6" />
    </test>
    <test name="regex-test-6a-2">
      <keystroke key="6" />
      <keystroke key="e" />
      <keystroke key="u-0320" />
      <keystroke key="stampy" />
      <check result="YES6c" />
      <backspace />
      <check result="YES6" />
    </test>
  </tests>
  <tests name="regex-tests-7">
    <test name="regex-test-7a-0">
      <keystroke key="stampy" />
      <keystroke key="7" />
      <keystroke key="e" />
      <keystroke key="u-0300" />
      <backspace /> <!-- skip the number, match any -->
      <check result="YES7" />
    </test>
    <!-- TODO: all the variations -->
  </tests>
  <tests name="regex-tests-8">
    <test name="regex-test-8a-0">
      <!-- simple test, exactly as written-->
      <keystroke key="8" />
      <keystroke key="e" />
      <keystroke key="stampy"/>
      <keystroke key="u-0300"/>
      <keystroke key="lgtm"/>
      <keystroke key="u-0320"/>
      <check result="YES8a"/>
    </test>
    <test name="regex-test-8b-0">
      <!-- simple test, exactly as written-->
      <keystroke key="8" />
      <keystroke key="e" />
      <keystroke key="u-0300"/>
      <keystroke key="stampy"/>
      <keystroke key="lgtm"/>
      <keystroke key="u-0320"/>
      <check result="YES8b"/>
    </test>
    <test name="regex-test-8e-0">
      <!-- simple test, exactly as written-->
      <keystroke key="8" />
      <keystroke key="e" />
      <keystroke key="u-0300"/> <!-- denorm-->
      <keystroke key="u-0320"/>
      <keystroke key="stampy"/>
      <keystroke key="lgtm"/>
      <check result="8NOYESYES0"/>
    </test>
    <test name="regex-test-8e-1">
      <keystroke key="8" />
      <keystroke key="e" />
      <keystroke key="u-0320"/> <!-- NFD-->
      <keystroke key="u-0300"/>
      <keystroke key="stampy"/>
      <keystroke key="lgtm"/>
      <check result="8e\u{0320}\u{0300}"/>
    </test>
    <test name="regex-test-8f-0">
      <keystroke key="8" />
      <keystroke key="f" />
      <keystroke key="u-0300"/> <!-- denorm-->
      <keystroke key="u-0320"/>
      <keystroke key="stampy"/>
      <keystroke key="lgtm"/>
      <check result="8f\u{0300}\u{0320}"/>
    </test>
    <test name="regex-test-8f-1">
      <keystroke key="8" />
      <keystroke key="f" />
      <keystroke key="u-0320"/> <!-- NFD-->
      <keystroke key="u-0300"/>
      <keystroke key="stampy"/>
      <keystroke key="lgtm"/>
      <check result="YES8d"/>
    </test>
  </tests>
  <tests name="regex-tests-9">
    <test name="regex-test-9a">
      <keystroke key="9" />
      <keystroke key="a" />
      <check result="9e\u{0300}\u{0320}"/>
    </test>
    <test name="regex-test-9b">
      <keystroke key="9" />
      <keystroke key="a" />
      <check result="9e\u{0300}\u{0320}"/>
    </test>
    <test name="regex-test-9c">
      <keystroke key="9" />
      <keystroke key="c" />
      <check result="9ce\u{0300}\u{0320}"/>
    </test>
    <test name="regex-test-9d">
      <keystroke key="9" />
      <keystroke key="d" />
      <check result="YES9d"/>
    </test>
  </tests>
  <!-- separate tests for NFC-->
  <tests name="regex-tests-9e">
    <test name="regex-test-9e-0">
      <keystroke key="9" />
      <keystroke key="e" />
      <keystroke key="e" />
      <keystroke key="u-0300" />
      <keystroke key="stampy" />
      <keystroke key="e" />
      <keystroke key="u-0300" /> <!-- out of order -->
      <keystroke key="u-0320" />
      <keystroke key="lgtm" />
      <check result="9ee\u{0300}e\u{0300}\u{0320}" />
    </test>
    <test name="regex-test-9e-0">
      <keystroke key="9" />
      <keystroke key="e" />
      <keystroke key="e" />
      <keystroke key="u-0300" />
      <keystroke key="stampy" />
      <keystroke key="e" />
      <keystroke key="u-0320" />
      <keystroke key="u-0300" /> <!-- in order -->
      <keystroke key="lgtm" />
      <check result="9ee\u{0300}e\u{0320}\u{0300}" />
    </test>
  </tests>
