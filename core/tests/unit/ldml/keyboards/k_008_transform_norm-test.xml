      <startContext to="" />
      <keystroke key="e" />
      <keystroke key="u-0300" />
      <keystroke key="u-0320" />
      <check result="e\u{0300}\u{0320}" />
    </test>
    <test name="regex-test-0-b">
      <startContext to="" />
      <keystroke key="e" />
      <keystroke key="u-0320" />
