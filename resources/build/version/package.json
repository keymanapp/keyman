{
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^2.1.0",
    "typescript": "^3.7.5",
    "yargs": "^15.1.0"
  },
  "description": "Automatically updates HISTORY.md based on pull requests",
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@types/semver": "^7.1.0",
    "semver": "^7.1.2",
    "ts-node": "^8.6.2"
  },
  "engines": {
    "node": "v16"
  },
  "files": [
    "dist",
    "src"
  ],
