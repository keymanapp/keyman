store(&VERSION) '9.0'
store(&NAME) 'Caps On Only'
begin Unicode > use(main)
  
caps on only

group(main) using keys


+ [NCAPS K_A] > 'ncaps a '
+ [CAPS K_A] > 'caps a '
+ [NCAPS SHIFT K_A] > 'ncaps shift a '
+ [CAPS SHIFT K_A] > 'caps shift a '
+ [SHIFT K_B] > 'shift b '
+ [K_B] > 'unmodified b '

c test a, b and c
