+ [ALT K_A] > 'ok'
+ [LCTRL K_A] > 'ok'
+ [RCTRL K_A] > 'ok'
+ [LALT K_A] > 'ok'
+ [RALT K_A] > 'ok'
+ [NCAPS K_A] > 'ok'
+ [CAPS K_A] > 'ok'
