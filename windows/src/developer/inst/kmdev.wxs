                      Maximum="11.0.0.0" IncludeMaximum="no" MigrateFeatures="yes" Property="VERSION10FOUND" />
    </Upgrade>

    <!-- Upgrade from Developer 11.0 or later versions -->

    <Upgrade Id="{E99A1F1E-7046-44F9-89EF-D3A59F84B209}">
