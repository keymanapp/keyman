c Description: Tests basic option rules with save reset+reset
c keys: [K_A][K_B][K_B][K_A]
c expected: foo.foo.
c context:
c option: foo=1
c expected option: foo=2

store(&version) '10.0'

store(foo) '0'

begin Unicode > use(Main)

group(Main) using keys

if(foo = '1') + 'a' > 'foo.' set(foo='2')
+ 'b' > reset(foo)
if(foo = '2') + 'a' > 'bar.'
if(foo = '0') + 'a' > 'no foo.'
