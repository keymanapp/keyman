{
  "name": "@keymanapp/common-types",
  "description": "Keyman Developer keyboard file types",
  "keywords": [
    "keyboard",
    "keyman",
    "ldml",
    "unicode"
  ],
  "type": "module",
  "exports": {
    ".": "./build/src/main.js"
  },
  "files": [
    "/build/src/"
  ],
  "scripts": {
    "build": "tsc -b",
