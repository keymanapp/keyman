    "prepublishOnly": "npm run build"
  },
  "author": "Marc Durdin <marc@keyman.com> (https://github.com/mcdurdin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keymanapp/keyman/issues"
  },
  "dependencies": {
    "@keymanapp/keyman-version": "*",
    "ajv": "^8.11.0",
    "restructure": "git+https://github.com/keymanapp/dependency-restructure.git#49d129cf0916d082a7278bb09296fb89cecfcc50",
    "semver": "^7.5.2",
    "xml2js": "git+https://github.com/keymanapp/dependency-node-xml2js#535fe732dc408d697e0f847c944cc45f0baf0829"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/git-diff": "^2.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.6",
    "@types/semver": "^7.3.12",
    "@types/xml2js": "^0.4.5",
    "c8": "^7.12.0",
    "chai": "^4.3.4",
    "chalk": "^2.4.2",
    "git-diff": "^2.0.6",
    "hexy": "^0.3.4",
    "mocha": "^8.4.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.9.5"
  },
  "mocha": {
    "spec": "build/test/**/test-*.js",
    "require": [
      "source-map-support/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keymanapp/keyman.git"
  }
}
