package radb4;

{$R *.RES}
{$ALIGN ON}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $00400000}
{$DESCRIPTION 'JVCL DB aware'}
{$RUNONLY}
{$IMPLICITBUILD ON}

requires
  vcl40,
  VCLDB40;

contains
  JvDBUtil in 'JvDBUtil.pas',
  JvDBLookupTreeView in 'JvDBLookupTreeView.pas',
  JvDBMove in 'JvDBMove.pas',
  JvDBTreeView in 'JvDBTreeView.pas',
  JvDBConst in 'JvDBConst.pas',
  JvSQLS in 'JvSQLS.pas';

end.
