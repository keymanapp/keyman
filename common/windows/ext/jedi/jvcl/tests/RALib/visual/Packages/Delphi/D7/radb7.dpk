package radb7;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'JVCL DB aware (run-time)'}
{$RUNONLY}
{$IMPLICITBUILD ON}

requires
  vcl,
  vcldb,
  bdertl;

contains
  JvDBUtil in '..\..\..\Source\JvDBUtil.pas',
  JvDBLookupTreeView in '..\..\..\Source\JvDBLookupTreeView.pas',
  JvDBMove in '..\..\..\Source\JvDBMove.pas',
  JvDBTreeView in '..\..\..\Source\JvDBTreeView.pas',
  JvSQLS in '..\..\..\Source\JvSQLS.pas',
  JvDBConst in '..\..\..\Source\JvDBConst.pas';

end.
