REGEDIT4

[HKEY_CLASSES_ROOT\.xyz]
@="XYZfile"

[HKEY_CLASSES_ROOT\XYZfile]
@="Demo file - MyApp"
"EditFlags"=hex:00,00,00,00

[HKEY_CLASSES_ROOT\XYZfile\Shell]
@=""

[HKEY_CLASSES_ROOT\XYZfile\Shell\open\command]
@="C:\\AppDdeCmdExample %1"

[HKEY_CLASSES_ROOT\XYZfile\Shell\open\ddeexec]
@="[Open(\"%1\")]"

[HKEY_CLASSES_ROOT\XYZfile\Shell\print]
"EditFlags"=hex:01,00,00,00

[HKEY_CLASSES_ROOT\XYZfile\Shell\print\command]
@="C:\\AppDdeCmdExample %1"

[HKEY_CLASSES_ROOT\XYZfile\Shell\print\ddeexec]
@="[Print(\"%1\", 2)]"

