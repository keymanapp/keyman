<?xml version="1.0" encoding="UTF-8"?>

<keyboard3 xmlns="https://schemas.unicode.org/cldr/45/keyboard3" locale="mt" conformsTo="45">
  <info name="vars-maximal"/>

  <keys />

  <!-- from spec -->
  <variables>
    <string id="affirmative" value="Yes" />
    <string id="yes" value="${affirmative}!" /> <!-- a simple string-->
    <set id="upper" value="A B C D E FF ${affirmative}"/>
    <set id="lower" value="a b c" />
    <set id="lowupp" value="$[upper] $[lower] Z"/>
    <uset id="consonants" value="[कसतनमह]" />
    <uset id="mixture" value="[[abc]$[consonants]]" />
  </variables>
</keyboard3>
