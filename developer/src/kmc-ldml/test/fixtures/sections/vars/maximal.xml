<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE keyboard3 SYSTEM "../../../../../../../resources/standards-data/ldml-keyboards/techpreview/dtd/ldmlKeyboard3.dtd">
<keyboard3 locale="mt" conformsTo="techpreview">
  <info name="vars-maximal"/>

  <keys />

  <!-- from spec -->
  <variables>
    <string id="affirmative" value="Yes" />
    <string id="yes" value="${affirmative}!" /> <!-- a simple string-->
    <set id="upper" value="A B C D E FF ${affirmative}"/>
    <set id="lower" value="a b c" />
    <set id="lowupp" value="$[upper] $[lower] Z"/>
    <unicodeSet id="consonants" value="[कसतनमह]" />
    <unicodeSet id="mixture" value="[[abc]$[consonants]]" />
  </variables>
</keyboard3>
