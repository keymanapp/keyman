<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE keyboard SYSTEM "../../../../../../../resources/standards-data/ldml-keyboards/techpreview/ldmlKeyboard.dtd">
<keyboard locale="mt" conformsTo="techpreview">
  <names>
    <name value="keys-minimal" />
  </names>

  <keys>
    <key id="grave" to="`" />
    <key id="one" to="1" />
    <key id="two" to="2" />
    <key id="three" to="3" />
    <key id="four" to="4" />
    <key id="five" to="5" />
    <key id="six" to="6" />
    <key id="seven" to="7" />
    <key id="eight" to="8" />
    <key id="nine" to="9" />
    <key id="zero" to="0" />
    <key id="ten" to="10" />
    <key id="eleven" to="11" />
    <key id="minus" to="-" />
    <key id="equal" to="=" />
  </keys>

  <layers form="hardware">
    <layer id="base">
      <!-- beware: this is mapping ` and 1! -->
      <row keys="grave one two three four five six seven eight nine ten eleven minus equal" />
    </layer>
  </layers>
</keyboard>
